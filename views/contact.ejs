<%- include("./partials/html-header")%>
<body class="contact-page" oncut="return false" oncopy="return false">
  <%- include("./partials/site-header")%>
  <div class="page-header">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="entry-header">
            <h2 class="entry-title">Contact</h2>
            <ul class="breadcrumbs flex align-items-center">
              <li><a href="#">Home</a></li>
              <li>Contact</li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="main-content">
      <div class="get-in-touch">
        <div class="row">
          <div class="col-12">
            <div class="the-complete-lineup">
              <div class="entry-title"><h2>Faculty Coordinator</h2></div>
              <div class="row the-complete-lineup-artists">
                <div class="col-6 col-md-4 col-lg-3 artist-single">
                  <h2 style="width:110%;">Prof. Rohit Mukherjee</h2>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <div class="the-complete-lineup">
              <div class="entry-title"><h2>Technical Team</h2></div>
              <div class="row the-complete-lineup-artists">
                <div class="col-6 col-md-4 col-lg-3 artist-single">
                  <h2>Web Development</h2>
                  <figure class="featured-image">
                    <img src="images/team/<%=team[0].name %>.jpg" alt="" />
                    <a href="#"> <img src="images/instagram.svg" alt="" /> </a>
                  </figure>
                  <h2><%=team[0].name %></h2>
                  <h2>+91 <%=team[0].contact %></h2>
                </div>
                <div class="col-6 col-md-4 col-lg-3 artist-single">
                  <h2>Designing & Graphics</h2>
                  <figure class="featured-image">
