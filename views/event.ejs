<%- include("./partials/html-header")%><body class="events-page"> <%- include("./partials/site-header")%> <div class="page-header"> <div class="container"> <div class="row"> <div class="col-12"> <div class="entry-header"> <h2 class="entry-title">Register</h2> <ul class="breadcrumbs flex align-items-center"></ul> </div></div></div></div></div><div class="page-container"> <div class="accordion-and-tabs-wrap"> <div class="entry-content"> <div class="holder"> <div class="side-column"> <div class="accordion-wrap type-accordion"> <h3 class="entry-title active flex justify-content-between list-title" > Technical Event<span class="arrow-r"></span> </h3> <div class="entry-content"> <ul class="list-holder"> <li><a href="#automaton">Automaton</a></li><% for(var i=0; i < EventList.Technical.length; i++){%> <li> <a href="#<%=EventList.Technical[i].tag %>" ><%=EventList.Technical[i].name %></a > </li><%}%> </ul> </div><h3 class="entry-title flex justify-content-between list-title"> Non-Technical Events<span class="arrow-r"></span> </h3> <div class="entry-content"> <ul class="list-holder"> <% for(var i=0; i < EventList.Non_Technical.length; i++){%> <li> <a href="#<%=EventList.Non_Technical[i].tag %>" ><%=EventList.Non_Technical[i].name %></a > </li><%}%> </ul> </div><h3 class="entry-title flex justify-content-between list-title"> Cultural Events<span class="arrow-r"></span> </h3> <div class="entry-content"> <ul class="list-holder"> <% for(var i=0; i < EventList.Cultural.length; i++){%> <li> <a href="#<%=EventList.Cultural[i].tag %>" ><%=EventList.Cultural[i].name %></a > </li><%}%> </ul> </div><h3 class="entry-title flex justify-content-between list-title"> Literary Events<span class="arrow-r"></span> </h3> <div class="entry-content"> <ul class="list-holder"> <% for(var i=0; i < EventList.Literary.length; i++){%> <li> <a href="#<%=EventList.Literary[i].tag %>" ><%=EventList.Literary[i].name %></a > </li><%}%> </ul> </div><h3 class="entry-title flex justify-content-between list-title"> Social Events<span class="arrow-r"></span> </h3> <div class="entry-content"> <ul class="list-holder"> <% for(var i=0; i < EventList.Social.length; i++){%> <li> <a href="#<%=EventList.Social[i].tag %>" ><%=EventList.Social[i].name %></a > </li><%}%> </ul> </div><h3 class="entry-title flex justify-content-between list-title"> Sports<span class="arrow-r"></span> </h3> <div class="entry-content"><ul class="list-holder"></ul></div></div></div><% for(var i=0; i < EventList.Technical.length; i++){%> <div class="detail-format" style="width: 90%; margin:auto;" id="<%=EventList.Technical[i].tag %>" > <h2 class="title"><%=EventList.Technical[i].name %></h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Technical[i].tag %>_notice" > Event Notice </li><% if(EventList.Technical[i].poster==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Technical[i].tag %>_poster" > Technical Event Poster </li><%}%> <% if(EventList.Technical[i].registration==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Technical[i].tag %>_reg" > Event Registration </li><%}%> </ul> <div class="tabs-container"> <div id="<%=EventList.Technical[i].tag %>_notice" class="tab-content" > <h2 class="entry-header">Event Notice</h2> <a href="./images/Event Captures/Technical/Notice/<%=EventList.Technical[i].name %> O.jpg" > <img src="./images/Event Captures/Technical/Notice/<%=EventList.Technical[i].name %>.jpg" alt="" class="post"/> </a> </div><% if(EventList.Technical[i].poster==true){%> <div id="<%=EventList.Technical[i].tag %>_poster" class="tab-content" > <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Technical/Poster/<%=EventList.Technical[i].name %> O.jpg" > <img src="./images/Event Captures/Technical/Poster/<%=EventList.Technical[i].name %>.jpg" alt="" class="post"/> </a> </div><%}%> <% if(EventList.Technical[i].registration==true){%> <div id="<%=EventList.Technical[i].tag %>_reg" class="tab-content" > <h2 class="entry-header">Registration</h2> <iframe src="/event/form/<%=EventList.Technical[i].tag %>" frameborder="0" ></iframe> </div><%}%> </div></div></div><%}%> <% for(var i=0; i < EventList.Non_Technical.length; i++){%> <div class="detail-format" style="width: 90%; margin:auto;" id="<%=EventList.Non_Technical[i].tag %>" > <h2 class="title"><%=EventList.Non_Technical[i].name %></h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Non_Technical[i].tag %>_notice" > Event Notice </li><% if(EventList.Non_Technical[i].poster==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Non_Technical[i].tag %>_poster" > Event Poster </li><%}%> <% if(EventList.Non_Technical[i].registration==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Non_Technical[i].tag %>_reg" > Event Registration </li><%}%> </ul> <div class="tabs-container"> <div id="<%=EventList.Non_Technical[i].tag %>_notice" class="tab-content" > <h2 class="entry-header">Event Notice</h2> <a href="./images/Event Captures/Non_Technical/Notice/<%=EventList.Non_Technical[i].name %> O.jpg" > <img src="./images/Event Captures/Non_Technical/Notice/<%=EventList.Non_Technical[i].name %>.jpg" alt="" class="post"/> </a> </div><% if(EventList.Non_Technical[i].poster==true){%> <div id="<%=EventList.Non_Technical[i].tag %>_poster" class="tab-content" > <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Non_Technical/Poster/<%=EventList.Non_Technical[i].name %> O.jpg" > <img src="./images/Event Captures/Non_Technical/Poster/<%=EventList.Non_Technical[i].name %>.jpg" alt="" class="post"/> </a> </div><%}%> <% if(EventList.Non_Technical[i].registration==true){%> <div id="<%=EventList.Non_Technical[i].tag %>_reg" class="tab-content" > <h2 class="entry-header">Registration</h2> <iframe src="/event/form/<%=EventList.Non_Technical[i].tag %>" frameborder="0" ></iframe> </div><%}%> </div></div></div><%}%> <% for(var i=0; i < EventList.Literary.length; i++){%> <div class="detail-format" style="width: 90%; margin:auto;" id="<%=EventList.Literary[i].tag %>" > <h2 class="title"><%=EventList.Literary[i].name %></h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Literary[i].tag %>_notice" > Event Notice </li><% if(EventList.Literary[i].poster==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Literary[i].tag %>_poster" > Event Poster </li><%}%> <% if(EventList.Literary[i].registration==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Literary[i].tag %>_reg" > Event Registration </li><%}%> </ul> <div class="tabs-container"> <div id="<%=EventList.Literary[i].tag %>_notice" class="tab-content" > <h2 class="entry-header">Event Notice</h2> <a href="./images/Event Captures/Literary/Notice/<%=EventList.Literary[i].name %> O.jpg" > <img src="./images/Event Captures/Literary/Notice/<%=EventList.Literary[i].name %>.jpg" alt="" class="post"/> </a> </div><% if(EventList.Literary[i].poster==true){%> <div id="<%=EventList.Literary[i].tag %>_poster" class="tab-content" > <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Literary/Poster/<%=EventList.Literary[i].name %> O.jpg" > <img src="./images/Event Captures/Literary/Poster/<%=EventList.Literary[i].name %>.jpg" alt="" class="post"/> </a> </div><%}%> <% if(EventList.Literary[i].registration==true){%> <div id="<%=EventList.Literary[i].tag %>_reg" class="tab-content" > <h2 class="entry-header">Registration</h2> <iframe src="/event/form/<%=EventList.Literary[i].tag %>" frameborder="0" ></iframe> </div><%}%> </div></div></div><%}%> <% for(var i=0; i < EventList.Cultural.length; i++){%> <div class="detail-format" style="width: 90%; margin:auto;" id="<%=EventList.Cultural[i].tag %>" > <h2 class="title"><%=EventList.Cultural[i].name %></h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Cultural[i].tag %>_notice" > Event Notice </li><% if(EventList.Cultural[i].poster==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Cultural[i].tag %>_poster" > Event Poster </li><%}%> <% if(EventList.Cultural[i].registration==true){%> <li class="tab-nav flex justify-content-center align-items-center active" data-target="#<%=EventList.Cultural[i].tag %>_reg" > Event Registration </li><%}%> </ul> <div class="tabs-container"> <div id="<%=EventList.Cultural[i].tag %>_notice" class="tab-content" > <h2 class="entry-header">Event Notice</h2> <a href="./images/Event Captures/Cultural/Notice/<%=EventList.Cultural[i].name %> O.jpg" > <img src="./images/Event Captures/Cultural/Notice/<%=EventList.Cultural[i].name %>.jpg" alt="" class="post"/> </a> </div><% if(EventList.Cultural[i].poster==true){%> <div id="<%=EventList.Cultural[i].tag %>_poster" class="tab-content" > <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Cultural/Poster/<%=EventList.Cultural[i].name %> O.jpg" > <img src="./images/Event Captures/Cultural/Poster/<%=EventList.Cultural[i].name %>.jpg" alt="" class="post"/> </a> </div><%}%> <% if(EventList.Cultural[i].registration==true){%> <div id="<%=EventList.Cultural[i].tag %>_reg" class="tab-content" > <h2 class="entry-header">Registration</h2> <iframe src="/event/form/<%=EventList.Cultural[i].tag %>" frameborder="0" ></iframe> </div><%}%> </div></div></div><%}%> <div class="detail-format" style="width: 90%; margin:auto;" id="automaton" > <h2 class="title">Automaton</h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active check" data-target="#automaton_notice" > Event Notice </li><li class="tab-nav flex justify-content-center align-items-center active check" data-target="#automaton_poster" > Event Poster </li><li class="tab-nav flex justify-content-center align-items-center active check" data-target="#automaton_reg" > Registration </li></ul> <style>check{padding-left: 10px; padding-right: 10px;}</style> <div class="tabs-container"> <div id="automaton_notice" class="tab-content"> <h2 class="entry-header">Event Notice</h2> <a href="./images/Event Captures/Technical/Notice/Automaton O.jpg" > <img src="./images/Event Captures/Technical/Notice/Automaton.jpg" alt="" class="post"/> </a> </div><div id="automaton_poster" class="tab-content"> <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Technical/Poster/automaton1.jpg" > <img src="./images/Event Captures/Technical/Poster/automaton1.jpg" alt="" class="post"/> </a> <p></p><a href="./images/Event Captures/Technical/Poster/automaton2.jpg" > <img src="./images/Event Captures/Technical/Poster/automaton2.jpg" alt="" class="post"/> </a> <p></p><a href="./images/Event Captures/Technical/Poster/automaton3.jpg" > <img src="./images/Event Captures/Technical/Poster/automaton3.jpg" alt="" class="post"/> </a> <p></p><a href="./images/Event Captures/Technical/Poster/automaton4.jpg" > <img src="./images/Event Captures/Technical/Poster/automaton4.jpg" alt="" class="post"/> </a> <p></p></div><div id="automaton_reg" class="tab-content"> <h2 class="entry-header" style="position:relative;"> Registration </h2> <div> <button id="robocross">Robocross</button> <button id="cargomania">Cargomania</button> <button id="hackbizz">Hackbizz</button> </div><script>document .getElementById("robocross") .addEventListener("click", ()=>{document.getElementById("robo").style.display="block"; document.getElementById("cargo").style.display="none"; document.getElementById("hack").style.display="none";}); document .getElementById("cargomania") .addEventListener("click", ()=>{document.getElementById("robo").style.display="none"; document.getElementById("cargo").style.display="block"; document.getElementById("hack").style.display="none";}); document .getElementById("hackbizz") .addEventListener("click", ()=>{document.getElementById("robo").style.display="none"; document.getElementById("cargo").style.display="none"; document.getElementById("hack").style.display="block";}); </script> <div id="robo"> <iframe src="/event/form/robocross" frameborder="0" ></iframe> </div><div id="cargo"> <iframe src="/event/form/cargomania" frameborder="0" ></iframe> </div><div id="hack"> <iframe src="/event/form/hackbizz" frameborder="0"></iframe> </div></div></div></div></div><div class="detail-format" style="width: 90%; margin:auto;" id="bhagirath" > <h2 class="title">Bhagirath</h2> <div class="tabs"> <ul class="tabs-nav flex"> <li class="tab-nav flex justify-content-center align-items-center active check" data-target="#bhagirath_poster" > Event Poster </li></ul> <style>check{padding-left: 10px; padding-right: 10px;}</style> <div class="tabs-container"> <div id="bhagirath_poster" class="tab-content"> <h2 class="entry-header">Event Poster</h2> <a href="./images/Event Captures/Social/Poster/Bhagirath1 O.jpg" > <img src="./images/Event Captures/Social/Poster/Bhagirath1.jpg" alt="" class="post"/> </a> <p></p><a href="./images/Event Captures/Social/Poster/Bhagirath2 O.jpg" > <img src="./images/Event Captures/Social/Poster/Bhagirath2.jpg" alt="" class="post"/> </a> </div></div></div></div></div></div></div></div><style>.page-header{background: url(./images/events-header-bg.jpg); background-position: center;}.page-container{width: 100%; margin: 100px 100px 100px 0;}.holder{display: flex; width: 100%; flex-direction: row; justify-content: space-around;}.side-column{width: 30%; height: 100%; margin-left: 25px;}.list-title{max-width: 100% !important; font-size: 1.25rem !important;}.detail-format{max-width: 65%; height: 100%; flex: 1 0 auto; margin-top: 0% !important; font-family: "Open Sans", Helvetica; display: none;}.detail-format:target{display: block;}.detail-format .title{color: #2e2e2e; font-weight: 500; text-align: center;}.list-holder{list-style-type: none;}.list-holder li a{text-decoration: none; color: rgb(150, 141, 224);}.post{width: 100%; height: auto;}/* HERE: FORM */ form{text-align: center;}label{display: block; letter-spacing: 4px; padding-top: 30px; text-align: center;}label .label-text{color: #9b9b9b; cursor: text; font-size: 20px; line-height: 20px; text-transform: uppercase; -moz-transform: translateY(-34px); -ms-transform: translateY(-34px); -webkit-transform: translateY(-34px); transform: translateY(-34px); transition: all 0.3s;}label input{background-color: transparent; border: 0; border-bottom: 2px solid #4a4a4a; color: #2e2e2e; font-size: 36px; letter-spacing: -1px; outline: 0; padding: 5px 20px; text-align: center; transition: all 0.3s; width: 200px;}label input:focus{max-width: 100%; width: 400px;}label input:focus + .label-text{color: #2e2e2e; font-size: 13px; -moz-transform: translateY(-100px); -ms-transform: translateY(-100px); -webkit-transform: translateY(-100px); transform: translateY(-100px);}label input:valid + .label-text{font-size: 13px; -moz-transform: translateY(-100px); -ms-transform: translateY(-100px); -webkit-transform: translateY(-100px); transform: translateY(-100px);}button{background-color: #333333; border: 2px solid white; border-radius: 27px; color: white; cursor: pointer; font-size: 20px; margin-top: 20px; padding: 15px 30px; text-transform: uppercase; transition: all 200ms;}button:hover, button:focus{background-color: white; color: #333333; outline: 0;}@media (max-width: 800px){.holder{flex-direction: column; margin: 0;}.side-column{width: 90%; margin: auto;}.detail-format{max-width: 100vw; margin: 0;}}iframe{width: 100%; height: 100vh;}</style> <%- include("./partials/footer.ejs")%></body>