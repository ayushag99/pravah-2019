            margin-bottom: 150px;
          }
          .event-holder .pair {
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-orient: horizontal;
            -webkit-box-direction: normal;
            -ms-flex-direction: row;
            flex-direction: row;
            -webkit-box-pack: justify;
            -ms-flex-pack: justify;
            justify-content: space-between;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
            height: 350px;
            padding: 70px 30px;
          }
          .pair:nth-child(2) {
            padding-left: 0;
          }
          .event {
            cursor: pointer;
          }
          .event img {
            margin-left: auto;
            margin-right: auto;
            display: block;
            width: 55%;
            height: auto;
            -webkit-transition: all 0.3s ease-out;
            -o-transition: all 0.3s ease-out;
            transition: all 0.3s ease-out;
          }
          .event h3 {
            text-align: center;
            -webkit-transition: all 5s ease-out;
            -o-transition: all 5s ease-out;
            transition: all 5s ease-out;
            font-size: 25px;
            margin-top: 1em;
            color: #2e2e2e;
            font-weight: 400;
          }
          .event ul {
            display: none;
            list-style-type: none;
            color: #2e2e2e;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
            opacity: 0;
            padding-left: 10px;
            padding-right: 10px;
          }
          .event ul li {
            text-align: center;
            font-size: 18px;
          }
          .event:hover {
            -webkit-animation: hovery 0.5s ease-out;
            animation: hovery 0.5s ease-out;
            -webkit-transform: scale(0.8, 0.8);
            -ms-transform: scale(0.8, 0.8);
            transform: scale(0.8, 0.8);
          }
          .event:hover img {
            margin-top: -60px;
          }
          .event:hover ul {
            -webkit-animation: moveup 0.4s ease-out;
            animation: moveup 0.4s ease-out;
            margin-top: 0px;
            opacity: 1;
            display: block;
          }
          @-webkit-keyframes hovery {
            0% {
              -webkit-transform: scale(1, 1);
              transform: scale(1, 1);
            }
            30% {
              -webkit-transform: scale(1.2, 1.2);
              transform: scale(1.2, 1.2);
            }
            100% {
              -webkit-transform: scale(0.8, 0.8);
              transform: scale(0.8, 0.8);
            }
          }
          @keyframes hovery {
            0% {
              -webkit-transform: scale(1, 1);
              transform: scale(1, 1);
            }
            30% {
              -webkit-transform: scale(1.2, 1.2);
              transform: scale(1.2, 1.2);
            }
            100% {
              -webkit-transform: scale(0.8, 0.8);
              transform: scale(0.8, 0.8);
            }
          }
          @-webkit-keyframes moveup {
            0% {
              margin-top: 50px;
              opacity: 0;
            }
            100% {
              margin-top: 0px;
              opacity: 1;
              display: block;
            }
          }
          @keyframes moveup {
            0% {
              margin-top: 50px;
              opacity: 0;
            }
            100% {
              margin-top: 0px;
              opacity: 1;
              display: block;
            }
          }
          @media (max-width: 1000px) {
            .event-holder {
              -webkit-box-orient: vertical;
              -webkit-box-direction: normal;
              -ms-flex-direction: column;
              flex-direction: column;
            }
            .event-holder .pair {
