  <script>
    const eventDate = new Date("25 feb 2019 9:00 AM").getTime();
    setInterval(() => {
      let distance = eventDate - Date.now();
      document.querySelector(".dday").textContent = Math.abs(Math.floor(
        distance / (1000 * 60 * 60 * 24)
      )+1);
      document.querySelector(".dhour").textContent = Math.abs(Math.floor(
        (distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60)
      )+1);
      document.querySelector(".dmin").textContent = Math.abs(Math.floor(
        (distance % (1000 * 60 * 60)) / (1000 * 60)
      )+1);
      document.querySelector(".dsec").textContent = Math.abs(Math.floor(
        (distance % (1000 * 60)) / 1000
      )+1);
    }, 1000);</script
